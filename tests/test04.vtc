varnishtest "unleash vmod `.is_enabled` E2E"

server s1 {} -start

varnish v1 -vcl+backend {
	import unleash from "${vmod}";

    sub vcl_init {
        new c = unleash.client(
            url="http://localhost:4242",
            token="*:development.964a287e1b728cb5f4f3e0120df92cb5");
	}

	sub vcl_deliver {
		set resp.http.x-enabled = c.is_enabled(name="test-toggle", session_id="123456");
	}
} -start

client c1 {
    delay 0.5
	txreq -url "/"
	rxresp
	expect resp.http.x-enabled == "true"
} -run
